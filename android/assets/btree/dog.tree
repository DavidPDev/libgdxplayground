#
# Dog tree
#

# Alias definitions
import bark:"BarkTask"
import care:"CareTask"
import mark:"MarkTask"
import walk:"WalkTask"

# Tree definition (note that root is optional)
root
  selector
    parallel
      care urgentProb:0.8
      alwaysFail
        RestTask # fully qualified task
    sequence
      bark times:"uniform,1,5"
      walk
      BarkTask # fully qualified task
      mark
